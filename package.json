{
  "name": "ns-to-libre",
  "version": "1.0.1",
  "description": "Transfer Nightscout data to LibreView",
  "keywords": [
    "nightscout",
    "libre",
    "libreview",
    "librelink",
    "diabetes",
    "cgm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wirthus/ns-to-libre.git"
  },
  "homepage": "https://github.com/wirthus/ns-to-libre#readme",
  "author": "wirthus",
  "license": "MIT",
  "type": "module",
  "exports": "./dist/index.js",
  "files": [
    "./dist/index.js"
  ],
  "bin": {
    "ns-to-libre": "./dist/index.js",
    "ns2libre": "./dist/index.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup-node",
    "release": "release-it",
    "dev:ts": "ts-node src/index.ts",
    "dev:ts:watch": "tsnd --respawn --transpile-only --rs src/index.ts --dry-run",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "colors": "^1.4.0",
    "commander": "^12.0.0",
    "dayjs": "^1.11.10",
    "read-pkg": "^9.0.1",
    "uuid": "^9.0.1",
    "yaml": "^2.4.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.21",
    "@types/uuid": "^9.0.8",
    "@wirthus/tsconfig": "^1.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-wirthus": "^1.0.0",
    "release-it": "^17.1.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@8.15.4"
}
